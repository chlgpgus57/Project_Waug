<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<mapper namespace="wishlistMapper">
	<insert id="wishAdd" parameterType="WishListVO">
		insert into wishlist(w_num,goods_num, email) values(wish_seq.nextval, ${goods_num},${email})
	</insert>
  	<delete id="wishDel" parameterType="WishListVO">
  		delete wishlist where goods_num=${goods_num}
  	</delete>
</mapper>